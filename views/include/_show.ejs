<%for(var i=0; i<users.length; i++){%>
<tr>
    <td><%=i+1%></td>
    <td><%=users[i].name%></td>
    <td><%=users[i].gender%></td>
    <td><%=users[i].phone%></td>
    <td><%=users[i].email%></td>
    <td>
        <a href="/update?id=<%=users[i]._id%>"class="btn border-shadow update"><span class="text-gradient"><i class="far fa-edit"></i></span></a>
    </td>
    <td>
        <a href=""class="btn border-shadow delete" data-id=<%=users[i]._id%>><span class="text-gradient"><i class="fas fa-trash"></i></span></a>   
    </td>
</tr>
<%}%>